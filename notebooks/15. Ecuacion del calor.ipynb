    "function malla(L::Tuple{Float64,Float64},tf::Number;h::Number=1e-2,k::Number=1e-2)\n",
    "    X=[L[1]]\n",
    "    x=L[1]+k\n",
    "    while x<L[2]\n",
    "        push!(X,x)\n",
    "        x=x+k\n",
    "    end\n",
    "    T=[0.0]\n",
    "    t=0.0+h\n",
    "    while t<tf\n",
    "        push!(T,t)\n",
    "        t=t+h\n",
    "    end\n",
    "    return (T,X)\n",
    "end"

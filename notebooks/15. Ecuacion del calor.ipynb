    "H=logspace(-1,-4,25)\n",
    "for i in H\n",
    "    println(\"h=$i\")\n",
    "    println(\"parametro=$(.01*i/(.01^2))\")\n",
    "    M=calorabs(x->deltadir(x,inf=100),(-1.0,1.0),1.0,d=.01,h=i,k=0.01)\n",
    "    println(M[1:10,convert(Int64,size(M)[2]/2)])\n",
    "    println(\"\\n\\n\")\n",
    "end"
   ]

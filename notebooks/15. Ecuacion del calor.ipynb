    "    for i in 1:(length(T)-1)\n",
    "        B=vec(total[i,:])\n",
    "        C=[B[i-1] for i in 2:length(B)]\n",
    "        unshift!(C,0.0)\n",
    "        D=[B[i+1] for i in 1:(length(B)-1)]\n",
    "        push!(D,0.0)\n",
    "        u=B+(h*d/(k^2))*(C+D-2*B)\n",
    "        if test==true\n",
    "            @show i\n",
    "            @show B\n",
    "            @show C\n",
    "            @show D\n",
    "            @show u\n",
    "        end\n",
    "        total=vcat(total,u')\n",
    "    end\n",
    "    return total\n",
    "end\n",
    "M=calorabs(x->deltadir(x,inf=100),(-1.0,1.0),5.0,d=.0001,h=.1,k=0.01)"
   ]
